{
  "storage": {
    "cors": [
      {
        "origin": ["*"],
        "method": ["GET", "POST", "PUT", "DELETE"],
        "maxAgeSeconds": 3600
      }
    ]
  },
  "hosting": {
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cross-Origin-Opener-Policy",
            "value": "same-origin-allow-popups"
          },
          {
            "key": "Cross-Origin-Embedder-Policy",
            "value": "require-corp"
          }
        ]
      }
    ]
  }
}
